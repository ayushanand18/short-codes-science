   ],
   "source": [
    "t = np.linspace(0,2*np.pi,1000)\n",
    "x = 100*(1+np.cos(t))*np.cos(t)\n",
    "y = 100*(1+np.cos(t))*np.sin(t)\n",
    "plt.plot(x,y)\n",
    "plt.show()"
   ]
